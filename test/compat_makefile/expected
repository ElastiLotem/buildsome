.PHONY: default
T :=  default
$(T): foo.o moshe.a moshe.a.b escape_input

T :=  foo.o
$(T): foo.c /lib /lib/x86_64-linux-gnu /tmp /usr/bin/as /usr/bin/gcc /usr/include /usr/include/stdc-predef.h /usr/include/x86_64-linux-gnu /usr/lib /usr/lib/gcc/x86_64-linux-gnu /usr/lib/gcc/x86_64-linux-gnu/4.8 /usr/lib/gcc/x86_64-linux-gnu/4.8/cc1 /usr/lib/gcc/x86_64-linux-gnu/4.8/include /usr/lib/gcc/x86_64-linux-gnu/4.8/include-fixed /usr/lib/x86_64-linux-gnu /usr/local/include
	rm -f 'foo.o'
	gcc -c -o foo.o foo.c

T :=  moshe.a moshe.a.b
$(T): /bin/cat foo.c
	rm -f 'moshe.a'
	rm -f 'moshe.a.b'
	cat foo.c > moshe.a ; cat foo.c >"moshe.a.b"

T :=  escape_input
$(T): /bin/cat escape(bla)
	rm -f 'escape_input'
	cat escape\(bla\) > escape_input

T :=  escape(bla)
$(T):
	rm -f 'escape(bla)'
	echo hi > escape\(bla\)

